{
    "id": null,
    "title": "Service Overview",
    "tags": ["overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"$service\", env=\"$env\"}[5m]))",
            "legendFormat": "Requests/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "format": "short",
            "logBase": 1
          }
        ]
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"x": 8, "y": 0, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"$service\", env=\"$env\", status_code=~\"5..|4..\"}[5m]))",
            "legendFormat": "Errors/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "format": "short",
            "logBase": 1
          }
        ]
      },
      {
        "id": 3,
        "title": "P95 Latency",
        "type": "graph",
        "gridPos": {"x": 16, "y": 0, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{service=\"$service\", env=\"$env\"}[5m])) by (le))",
            "legendFormat": "P95 Latency"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "format": "s",
            "logBase": 1
          }
        ]
      },
      {
        "id": 4,
        "title": "Top Failing Routes",
        "type": "table",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "topk(10, sum by (route) (rate(http_requests_total{service=\"$service\", env=\"$env\", status_code=~\"5..|4..\"}[5m])))",
            "format": "table"
          }
        ]
      },
      {
        "id": 5,
        "title": "Slowest Routes",
        "type": "table",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "topk(10, avg by (route) (http_request_duration_seconds_sum{service=\"$service\", env=\"$env\"} / http_request_duration_seconds_count{service=\"$service\",\n env=\"$env\"}))",
            "format": "table"
          }
        ]
      },
      {
        "id": 6,
        "title": "Service Availability",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "avg(up{service=\"$service\", env=\"$env\"})",
            "legendFormat": "Up"
          }
        ]
      },
      {
        "id": 7,
        "title": "Requests by Status",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (status_code) (rate(http_requests_total{service=\"$service\", env=\"$env\"}[5m]))",
            "legendFormat": "{{status_code}}"
          }
        ]
      },
      {
        "id": 8,
        "title": "P95 Latency by Route",
        "type": "graph",
        "gridPos": {"x": 12, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{service=\"$service\", env=\"$env\"}[5m])) by (le, route))",
            "legendFormat": "{{route}}"
          }
        ]
      },
      {
        "id": 9,
        "title": "Scrape Samples by Service",
        "type": "graph",
        "gridPos": {"x": 0, "y": 32, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "sum by (service) (scrape_samples_post_metric_relabeling{env=\"$env\"})",
            "legendFormat": "{{service}}"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(service)",
          "refresh": 1,
          "options": [],
          "current": {
            "selected": true,
            "text": "node-express-example",
            "value": "node-express-example"
          }
        },
        {
          "name": "env",
          "type": "query",
          "query": "label_values(env)",
          "refresh": 1,
          "options": [],
          "current": {
            "selected": true,
            "text": "development",
            "value": "development"
          }
        }
      ]
    }
}
